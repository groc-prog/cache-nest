{
  "globalEnv": [
    "CI",
    "BASE_URL",
    "LOG_LEVEL",
    "NODE_ENV",
    "CACHE_NEST_VERSION",
    "HOSTNAME"
  ],
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "transit": {
      "dependsOn": [
        "^transit"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "transit"
      ]
    },
    "type-check": {
      "dependsOn": [
        "transit"
      ]
    },
    "test:unit": {
      "dependsOn": [
        "^test:unit"
      ]
    },
    "test:unit:watch": {
      "persistent": true,
      "cache": false,
      "dependsOn": [
        "^test:unit:watch"
      ]
    },
    "test:e2e": {
      "dependsOn": [
        "^test:e2e"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "preview": {
      "cache": false,
      "persistent": true
    },
    "//#format": {}
  }
}
