{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --loader=ts-node/esm --watch src/index.ts",
    "build": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --format=esm",
    "preview": "npm run build && node dist/index.js",
    "test:unit": "vitest --config vitest.unit.config.ts",
    "test:e2e": "vitest --config vitest.e2e.config.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix"
  },
  "dependencies": {
    "@cache-nest/types": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.52.1",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.52.1",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.52.1",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.52.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.1",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.52.1",
    "@opentelemetry/instrumentation-fs": "^0.14.0",
    "@opentelemetry/instrumentation-http": "^0.52.1",
    "@opentelemetry/instrumentation-winston": "^0.39.0",
    "@opentelemetry/resources": "^1.25.1",
    "@opentelemetry/sdk-metrics": "^1.25.1",
    "@opentelemetry/sdk-node": "^0.52.1",
    "@opentelemetry/sdk-trace-node": "^1.25.1",
    "@opentelemetry/semantic-conventions": "^1.26.0",
    "@opentelemetry/winston-transport": "^0.5.0",
    "async-mutex": "^0.5.0",
    "basic-auth": "^2.0.1",
    "check-disk-space": "^3.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "hyper-express": "^6.16.4",
    "js-yaml": "^4.1.0",
    "lodash-es": "^4.17.21",
    "object-hash": "^3.0.0",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cache-nest/eslint-config": "workspace:*",
    "@cache-nest/typescript-config": "workspace:*",
    "@types/cors": "^2.8.17",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.16.1",
    "@types/object-hash": "^3.0.6",
    "@vitest/coverage-v8": "^2.0.5",
    "esbuild": "^0.23.1",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5",
    "vitest": "^2.0.5"
  }
}
