{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun --format esm",
    "preview": "NODE_ENV=production npm run build && bun dist/index.js",
    "test:unit": "bun test --coverage test/*.{spec,test}.ts test/**/*.{spec,test}.ts",
    "test:unit:watch": "bun test --coverage --watch test/*.{spec,test}.ts test/**/*.{spec,test}.ts",
    "test:e2e": "vitest --config vitest.e2e.config.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix"
  },
  "dependencies": {
    "@cache-nest/types": "workspace:*",
    "@elysiajs/cors": "^1.1.1",
    "@elysiajs/opentelemetry": "^1.1.5",
    "@elysiajs/swagger": "^1.1.1",
    "@msgpack/msgpack": "3.0.0-beta2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.52.1",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.52.1",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.52.1",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.52.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.1",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.52.1",
    "@opentelemetry/instrumentation-fs": "^0.14.0",
    "@opentelemetry/instrumentation-http": "^0.52.1",
    "@opentelemetry/resources": "^1.25.1",
    "@opentelemetry/sdk-metrics": "^1.25.1",
    "@opentelemetry/sdk-node": "^0.52.1",
    "@opentelemetry/sdk-trace-node": "^1.25.1",
    "@opentelemetry/semantic-conventions": "^1.26.0",
    "@opentelemetry/winston-transport": "^0.5.0",
    "@sinclair/typebox": "^0.33.12",
    "async-mutex": "^0.5.0",
    "check-disk-space": "^3.4.0",
    "elysia": "^1.1.12",
    "elysia-autoload": "^1.4.0",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "lodash-es": "^4.17.21",
    "object-hash": "^3.0.0",
    "proper-lockfile": "^4.1.2",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cache-nest/eslint-config": "workspace:*",
    "@cache-nest/typescript-config": "workspace:*",
    "@types/bun": "^1.1.9",
    "@types/cors": "^2.8.17",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.16.1",
    "@types/object-hash": "^3.0.6",
    "@types/proper-lockfile": "^4.1.4",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  }
}
